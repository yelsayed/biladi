{
  "name": "Qaata3",
  "description": "Boycott evil organizations",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "static/js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "./images/danger38.png",
        "./images/success38.png"
      ]
    }
  ],
  "background": {
    "service_worker": "static/js/background.js"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}